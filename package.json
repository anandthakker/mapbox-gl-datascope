{
  "name": "mapbox-gl-datascope",
  "version": "0.0.0",
  "description": "A stopgap to mimic data-driven styles in mapbox-gl-js; hopefully obsolete very soon!",
  "keywords": [
    "mapbox",
    "mapbox-gl",
    "mapbox-gl-js",
    "data",
    "data-driven",
    "plugin"
  ],
  "main": "index.js",
  "scripts": {
    "retire": "retire -p",
    "lint": "eslint *.js test",
    "docs-readme": "documentation readme -s API",
    "docs-check": "documentation readme -s API -c",
    "test": "npm run retire && npm run lint && npm run docs-check && tap --cov test/*.js",
    "prepublish": "npm run bundle && npm run docs-readme",
    "start": "budo index.js:dist/mapbox-gl-datascope.js -- --standalone mapboxgl.datascope -g yo-yoify",
    "bundle": "browserify index.js -g yo-yoify --standalone mapboxgl.datascope > dist/mapbox-gl-datascope.js"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "budo": "^8.2.2",
    "documentation": "^4.0.0-beta2",
    "eslint": "^2.8.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "mapbox-gl-draw": "^0.6.0-rc9",
    "retire": "*",
    "tap": "^2.1.1",
    "yo-yoify": "^1.0.4"
  },
  "dependencies": {
    "d3-array": "^0.7.1",
    "yo-yo": "^1.2.0"
  }
}
